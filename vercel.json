{
    "version": 2,
    "builds": [
      {
        "src": "./app.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "src/app.js" }
  ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "source": "/(.*)",
            "headers": [
              { "key": "Access-Control-Allow-Origin", "value": "*" }
            ]
          },
          {
            "source": "/td-alpha.vercel.app/(.*)",
            "headers": [
              { "key": "Access-Control-Allow-Origin", "value": "*" }
            ]
          }  
      ]
      }
    ]
  }
  